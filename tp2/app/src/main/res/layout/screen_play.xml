<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/coordinatorLayout"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/etoilefond"
    tools:context=".MainActivity">

    <ImageView
        android:id="@+id/ast1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:srcCompat="@drawable/asteroid2" />

    <ImageView
        android:id="@+id/jOut"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_gravity="bottom|center"
        android:layout_marginBottom="0.25in"
        app:srcCompat="@drawable/pad_exterior" />

    <ImageView
        android:id="@+id/explosion"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:srcCompat="@drawable/explosion" />

    <ImageView
        android:id="@+id/jCent"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_gravity="bottom|center"
        android:layout_marginBottom="0.40in"
        app:srcCompat="@drawable/pad_center" />

    <ImageView
        android:id="@+id/tieFighter"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:srcCompat="@drawable/tie" />

</FrameLayout>